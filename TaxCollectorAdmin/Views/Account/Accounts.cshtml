
@{
    ViewData["Title"] = "Accounts";
}

@section css {
    <link href="~/css/account/accounts.css" rel="stylesheet" />
}


<div class="card">

    <div id="tccGridView">
        <div class="card-header">
            <div class="row">
                <div class="col-sm-3" id="">
                    <select type="text" id="listOfAssemblies" class="form-control" required></select>
                </div>
                <div class="col-sm-3">
                    <button type="button" id="btnSearch" class="btn btn-success btn-sm"><i class='fa fa-search'></i> Search</button>
                </div>
            </div>
        </div>
        <div class="card-body">
            <div class="row align-items-end" style="margin: 1px;">
                <div class="gridDiv">
                    <div id="grid"></div>
                </div>
            </div>
        </div>
    </div>

</div>

@section scripts {
    <script src="~/js/account/accounts.js"></script>
}

